    game.ReplicatedStorage.HandModels.USP9.Body.Transparency = Tr
    game.ReplicatedStorage.HandModels.USP9.Mag.Transparency = Tr
    --<Blunderbuss>--
    game.ReplicatedStorage.HandModels.Blunderbuss.Body.Transparency = Tr
    game.ReplicatedStorage.HandModels.Blunderbuss.thing.Transparency = Tr
    game.ReplicatedStorage.HandModels.Blunderbuss.Tube.Transparency = Tr
    --<GaussRifle>--
    game.ReplicatedStorage.HandModels.GaussRifle.DefaultSight.Sight2.Transparency = Tr
    game.ReplicatedStorage.HandModels.GaussRifle.DefaultSight.Sight1.Transparency = Tr
