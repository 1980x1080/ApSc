

function GunSystemTransparency(Tr)
    --<Bandage>--
    game.ReplicatedStorage.HandModels.Bandage.Bandage.Transparency = Tr
    --<PipeSMG>--
    game.ReplicatedStorage.HandModels.PipeSMG.DefaultSight.RearSight.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.DefaultSight.FrontSight.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Body.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Bolt.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Stock.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Grip.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Flap.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Mag.Transparency = Tr
    game.ReplicatedStorage.HandModels.PipeSMG.Barrel.Transparency = Tr
    --<PumpShotgun>--
    game.ReplicatedStorage.HandModels.PumpShotgun.RearSight.Transparency = Tr
    game.ReplicatedStorage.HandModels.PumpShotgun.Barrel.Transparency = Tr
    game.ReplicatedStorage.HandModels.PumpShotgun.MainMetal.Transparency = Tr
    game.ReplicatedStorage.HandModels.PumpShotgun.Slider.Transparency = Tr
    game.ReplicatedStorage.HandModels.PumpShotgun.Body.Transparency = Tr
    game.ReplicatedStorage.HandModels.PumpShotgun.Muzzle.Transparency = Tr
